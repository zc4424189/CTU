#ifndef CLOGS_H_
#define CLOGS_H_
#include "includs.h"

enum LOGLEVEL
{
      ERROR,
      NOTICE,
      WARN,
      INFO
};
class CLogs {

public:
	CLogs();
	virtual ~CLogs();

private:
	std::string Strnowtime;
	std::string Strmessage;
	LOGLEVEL loglevel;
	int GetTime();
	std::string AssembleMessage(LOGLEVEL AlarmClass);
};

#endif /* CLOGS_H_ */
